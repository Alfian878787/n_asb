From f05791524f2a57ae7473d1482a4e5fe9663cae58 Mon Sep 17 00:00:00 2001
From: Moritz Horstmann <dev@peterzweg.at>
Date: Sat, 13 Jan 2018 21:49:29 +0100
Subject: [PATCH] Bump security patch level to 2018-01-05

Change-Id: I1038e75a8b69b84da39b87ac067edceac8b2be87
---

diff --git a/core/version_defaults.mk b/core/version_defaults.mk
index e4077cf..68fcba2 100644
--- a/core/version_defaults.mk
+++ b/core/version_defaults.mk
@@ -131,7 +131,7 @@
     #  It must be of the form "YYYY-MM-DD" on production devices.
     #  It must match one of the Android Security Patch Level strings of the Public Security Bulletins.
     #  If there is no $PLATFORM_SECURITY_PATCH set, keep it empty.
-    PLATFORM_SECURITY_PATCH := 2017-12-05
+    PLATFORM_SECURITY_PATCH := 2018-01-05
 endif
 
 ifeq "" "$(PLATFORM_BASE_OS)"
